{
  "compilerOptions": {
    "target": "es2020",
    "module": "preserve",
    "allowJs": true,
    "checkJs": true,
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "resolveJsonModule": true,
    "strict": false,
    "noEmit": true
  },
  "include": ["src/**/*.js"],
  "exclude": ["node_modules", "build", "public"]
}
